@model HousingSManagement.Models.House

@{
    ViewData["Title"] = "Edit House";
}

<h2>Edit House</h2>

<form asp-action="Edit">
    <input type="hidden" asp-for="Id" />
    <div class="form-group">
        <label asp-for="OwnerName"></label>
        <input asp-for="OwnerName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="HouseNumber"></label>
        <input asp-for="HouseNumber" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Block"></label>
        <input asp-for="Block" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber"></label>
        <input asp-for="PhoneNumber" class="form-control" />
    </div>
    <button type="submit" class="btn btn-warning">Update</button>
    <a asp-action="Index" class="btn btn-secondary">Back</a>
</form>
